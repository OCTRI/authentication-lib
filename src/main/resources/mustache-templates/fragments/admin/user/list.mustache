<div class="container-fluid table-responsive">
	<h3>List of Users</h3>
	<table class="table table-striped table-bordered users-table center-all display responsive no-wrap">
		<thead>
			<th></th>
			<th>Username</th>
			<th>Roles</th>
			<th>Email</th>
			<th>First Name</th>
			<th>Last Name</th>
			<th>Account Enabled</th>
			<th>Account Locked</th>
			<th>Account Expired</th>
			<th>Credentials Expired</th>
		</thead>
		<tbody>
			{{#allUsers}}
			<tr>
				<td>
					<a href="{{req.contextPath}}/admin/user/form?id={{id}}">
						<span class="glyphicon glyphicon-pencil"></span>
					</a>
				</td>
				<td>
					{{username}}
				</td>
				<td>
					{{#userRoles}}
					<p>{{description}}</p>
					{{/userRoles}}
				</td>
				<td>
					{{email}}
				</td>
				<td>
					{{firstName}}
				</td>
				<td>
					{{lastName}}
				</td>
				<td>
					{{#enabled}}
					<span class="glyphicon glyphicon-ok text-success"></span>
					{{/enabled}}
				</td>
				<td>
					{{#accountLocked}}
					<span class="glyphicon glyphicon-ok text-success"></span>
					{{/accountLocked}}
				</td>
				<td>
					{{#accountExpired}}
					<span class="glyphicon glyphicon-ok text-success"></span>
					{{/accountExpired}}
				</td>
				<td>
					{{#credentialsExpired}}
					<span class="glyphicon glyphicon-ok text-success"></span>
					{{/credentialsExpired}}
				</td>
			</tr>
			{{/allUsers}}
		</tbody>
	</table>
	</div>